{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Principal": {"Service": "s3.amazonaws.com"},
      "Action": "s3:ListBucket",
      "Resource": "arn:aws:s3:::us-west-2-metaforager-terraform"
    },
    {
      "Effect": "Allow",
      "Principal": {"Service": "s3.amazonaws.com"},
      "Action": ["s3:GetObject", "s3:PutObject"],
      "Resource": "arn:aws:s3:::us-west-2-metaforager-terraform/*"
    }
  ]
}


